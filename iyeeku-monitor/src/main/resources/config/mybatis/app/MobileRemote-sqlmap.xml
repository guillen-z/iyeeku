<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iyeeku.remote.dao.IMobileRemoteDao">

    <select id="findMonitoredMachineListByDeviceId" parameterType="java.lang.String" resultType="java.util.HashMap">
        select zj,jqbh,macdz,dnmc,czxt,jqzxzt from PC_MONITOR
    </select>

    <select id="findMonitoredMachineInfoByZj" parameterType="java.lang.String" resultType="java.util.HashMap">
        select zj,jqbh,macdz,dnmc,czxt,jqzxzt from PC_MONITOR WHERE zj = #{VALUE}
    </select>

</mapper>